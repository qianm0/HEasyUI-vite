#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/lesson_qianmo/Vue/heasy-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/lesson_qianmo/Vue/heasy-ui-vite/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+update-browserslist-db@1.0.6_browserslist@4.21.3/node_modules/update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+update-browserslist-db@1.0.6_browserslist@4.21.3/node_modules/update-browserslist-db/cli.js" "$@"
fi
